/* 全局样式 */
.container {
  height: 100%;
  /*display: -webkit-flex;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;*/
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

.h1 { 
	line-height: 1.2;
}
.flex-view, .flex-view view {
	display: -webkit-flex;
  display: flex;
}
.flex-column {
	-webkit-flex-direction: column;
  flex-direction: column;
}
image { 
	width: 100%; max-height: 440rpx; 
}

/* 搜索框 */
.search-view { 
	width: 100%; height: 80rpx;
}
.search-pad { 
	position: fixed; 
	top: 0;
	z-index: 10;
	width: 100%; 
	height: 80rpx;
	background:#f3f3f3; 
	display: flex;
	align-items: center; 
}
.search-pad .city { 
    flex-basis:200rpx;
    color:#000;
    display:flex;
    flex-direction:column;
    justify-content:center;
    margin-left:20rpx;
    height:50rpx;    
    flex-grow: 1;
    text-align: center;
}
.search {
    background:#fff;
    border-radius:28rpx;
    height:56rpx;
    line-height: 56rpx;
    position: relative;
    margin-right: 20rpx;
    border: 1px solid #c9c9c9;
}
.search input { 
	width: 440rpx;
	padding-left: 10px;
	padding-right: 50rpx;
	line-height: 12px;
	padding-top:3px;
	font-size: 12px;
}
.search icon {
	font-weight:bold;
  position:absolute;
  top:6px;
  right: 10px
}

/* 轮播全局样式样式 */
.common-swiper {
  width: 100%;
  height: 440rpx;
  background: #f3f3f3;
}
.common-swiper image { width: 100%; height: inherit;}

/* 轮播图文字介绍 */
.trans-bg {
  width:100%;
  height:100rpx;
  background:rgba(0,0,0,.3);
  position:relative;
  top:-100rpx;
  color:#fff;
  display:flex;
  margin:auto;
  align-items:center;
}
.trans-bg text { padding: 0 20px; font-weight: 700; line-height: 1.2}

/* 轮播图小圆点样式覆盖 */
div.wx-swiper-dots.wx-swiper-dots-horizontal { height: 20px; }
div.wx-swiper-dot.wx-swiper-dot-active::before { background-color: #fff !important;} 

/* 全局字体大小定义 */
.h1 { font-size: 24px; font-weight: 700;}
.h2 { font-size: 20px; font-weight: 700;}
.small { font-size: 14px; }
.tiny { font-size: 10px; }

/* 标签样式 */
.tag-container { margin: 20rpx 0;}
.tag {
	width:auto;
	background-color: #E8E8E8;
	border-color: #E8E8E8;
	color:rgba(0,0,0,.6);
	border-radius:0 4px 4px 0;
	display:inline-block;
	padding:10rpx 40rpx;
	margin-bottom:20rpx;
	position:relative;
	margin-left:40rpx;
	margin-right:20rpx;
	height:50rpx;
	line-height:50rpx;
}
.tag:before {
  content:'';
  position: absolute;
  transform: translateY(-50%) translateX(50%) rotate(-45deg);
  top: 50%;
  right: 100%;
  background-color: inherit;
  background-image: none;
  width: 50rpx;
  height: 49rpx;
  transition: none;
}
.tag:after {
  content: "";
  position: absolute;
  width: 12rpx;
  height: 12rpx;
  top: 50%;
  transform: translateY(-50%);
  left: 8rpx;
  border: 1rpx solid #9a9a9a;

  background: #fff;
  border-radius: 50%;
}

.phone, .highlight { color: #0084B4;}

/* 详情页公共布局 */
.detail {  display: flex; padding: 20rpx; flex-direction: column;}
.detail .img-container { width: 100%; height: auto; display: flex;}
.detail .img-container image { width: 100%; margin: 0 auto; height: 440rpx; background-color: #eeeeee;}
.detail .headline { padding: 30rpx 0;}
.detail .headline .h1 { display: block; margin-bottom: 10rpx;}

.text-container .wxParse-p {
	line-height: 1.4;
}


.article-detail .detail-container { padding: 0 20rpx; }